<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	
	<navigation-rule>
	
		<from-view-id>/pages/private/funcionalidades/minkoski.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>minkoski2</from-outcome>
			<to-view-id>/pages/private/funcionalidades/minkoski2.xhtml</to-view-id>
		</navigation-case>
		
	</navigation-rule>
	
	<navigation-rule>
	
		<from-view-id>/pages/private/funcionalidades/upload.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>uploadok</from-outcome>
			<to-view-id>/pages/private/funcionalidades/multobj.xhtml</to-view-id>
		</navigation-case>
		
	</navigation-rule>
	
	<navigation-rule>
	
		<from-view-id>/pages/private/funcionalidades/multobj.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>ok</from-outcome>
			<to-view-id>/pages/private/funcionalidades/peso.xhtml</to-view-id>
		</navigation-case>
		
	</navigation-rule>
	
	<navigation-rule>
	
		<from-view-id>/pages/private/funcionalidades/peso.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>pesook</from-outcome>
			<to-view-id>/pages/private/funcionalidades/chamardistancias.xhtml</to-view-id>
		</navigation-case>
		
	</navigation-rule>

	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>

</faces-config>